pipeline{
    agent any
    environment {
        Image_Name = "fadysg/go_go"
    }
    stages {
        stage('Build') {
            steps{
                script {
                    dockerImage = docker.build(Image_Name + ":$BUILD_NUMBER")
                }
            }
            post{
                success {
                    echo 'Docker image built successfully.'
                }
                failure {
                    echo 'Docker image build failed.'
                }
            }
        }
    }
}